<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN"
"http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">
<liferay-portlet-app>
   <portlet>
    <portlet-name>ApplicationRegistryFull</portlet-name>
    <header-portlet-css>/ui/css/smoothness/jquery-ui-1.8.12.custom.css</header-portlet-css>
    <header-portlet-css>/datatables/extras/TableTools/media/css/TableTools.css</header-portlet-css>
    <header-portlet-css>/datatables/extras/TableTools/media/css/TableTools_JUI.css</header-portlet-css>
    <header-portlet-css>/datatables/media/css/demo_page.css</header-portlet-css>
    <header-portlet-css>/datatables/media/css/demo_table.css</header-portlet-css>
    <header-portlet-css>/datatables/media/css/demo_table_jui.css</header-portlet-css>
    <header-portlet-css>/ui/development-bundle/demos/demos.css</header-portlet-css>
    <header-portlet-css>/ui/css/imagepreview.css</header-portlet-css>
    <header-portlet-javascript>/datatables/jquery-1.6.min.js</header-portlet-javascript>
    <header-portlet-javascript>/ui/js/jquery-ui-1.8.12.custom.min.js</header-portlet-javascript>
    <header-portlet-javascript>https://www.google.com/jsapi</header-portlet-javascript>
    <header-portlet-javascript>/js/google.js</header-portlet-javascript>
    <header-portlet-javascript>/js/imgpreview.js</header-portlet-javascript>
    <header-portlet-javascript>/datatables/media/js/jquery.dataTables.min.js</header-portlet-javascript>
    <header-portlet-javascript>/datatables/extras/FixedHeader/js/FixedHeader.min.js</header-portlet-javascript>
    <header-portlet-javascript>/datatables/extras/TableTools/media/js/ZeroClipboard.js</header-portlet-javascript>
    <header-portlet-javascript>/datatables/extras/TableTools/media/js/TableTools.min.js</header-portlet-javascript>

   </portlet>
   <role-mapper>
      <role-name>administrator</role-name>
      <role-link>Administrator</role-link>
   </role-mapper>
   <role-mapper>
      <role-name>guest</role-name>
      <role-link>Guest</role-link>
   </role-mapper>
   <role-mapper>
      <role-name>power-user</role-name>
      <role-link>Power User</role-link>
   </role-mapper>
   <role-mapper>
      <role-name>user</role-name>
      <role-link>User</role-link>
   </role-mapper>
</liferay-portlet-app>
